<div class="modal fade" id="contact_modal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title" id="exampleModalLabel">Submit Details</h3>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">Ã—</span>
                </button>
            </div>
            <form id="submit_form">
                <div class="modal-body">
                    <div class="form-group">
                        <label>Your Name</label>
                        <input type="text" class="form-control" id="submit_name" required="">
                    </div>
                    <div class="form-group">
                        <label>Your Email</label>
                        <input type="email" class="form-control" id="submit_email" required="">
                    </div>
                    <div class="form-group">
                        <label>Domain</label>
                        <input type="text" class="form-control" id="submit_domain" required="">
                    </div>
                    <div class="form-group">
                        <label>Phone Number</label>
                        <input type="text" class="form-control" id="submit_phone" required="">
                    </div>
                    <div class="form-group">
                        <label>Company Name</label>
                        <input type="text" class="form-control" id="submit_company" required="">
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-danger" style="background: red;">Submit</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                </div>
            </form>
        </div>
    </div>
</div>
<script>
    $('#submit_form').on('submit', function (e) {
        e.preventDefault();
        $('#submit_form button[type="submit"]').attr("disabled", true);
        $.ajax({
            url: "/submit-contact",
            method: "post",
            data: {
                type: "home",
                name: $('#submit_name').val(),
                email: $('#submit_email').val(),
                domain: $('#submit_domain').val(),
                phone: $('#submit_phone').val(),
                company: $('#submit_company').val(),
            },
            success: function (res) {
                if (res.status === 'success') {
                    customAlert(res.message, true);
                    $('#contact_modal').modal('toggle');
                }
                else customAlert(res.message);
                $('#submit_form button[type="submit"]').attr("disabled", false);
            }
        })
    })
</script>
